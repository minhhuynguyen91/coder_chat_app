<h1>Sign up</h1>
<br.
<div class="row">
  <div class=".col-12 .col-sm-6 .col-lg-8"> 
    <%= form_tag signin_path, method: 'post' do %>
      <div class="form-group">
          <%= label_tag :Email %>
          <%= text_field_tag :email, "", class: 'form-control' %>
      </div>
    
      <div class="form-group">
          <%= label_tag :password %>
          <%= password_field_tag :password, "", class: 'form-control' %>
      </div>
    
      <%= submit_tag 'Sign In' %>
    
    <% end %>
 </div>
</div>